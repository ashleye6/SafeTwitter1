


<%= form_for @blockedshow do |b| %>
    <%= b.label :tvshow_id, "Add a Blocked TV Show " %><br/>
    <%= collection_select(:blockedshow, :tvshow_id, Tvshow.all, :id, :title, prompt: true) %>

    <%= submit_tag 'Add' %>
<% end %>







